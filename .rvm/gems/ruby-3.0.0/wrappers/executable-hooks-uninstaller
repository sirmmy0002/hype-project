#!/usr/bin/env bash

if
  [[ -s "/home/sirmmy/.rvm/gems/ruby-3.0.0/environment" ]]
then
  source "/home/sirmmy/.rvm/gems/ruby-3.0.0/environment"
  exec executable-hooks-uninstaller "$@"
else
  echo "ERROR: Missing RVM environment file: '/home/sirmmy/.rvm/gems/ruby-3.0.0/environment'" >&2
  exit 1
fi
